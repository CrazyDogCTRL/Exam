// –ì—Ä–∞—Ñ–∏–∫–∏ —Ñ—É–Ω–∫—Ü–∏–π, –≤—ã—á–∏—Å–ª–µ–Ω–∏—è –ø—Ä–∏–º–µ—Ä–æ–≤ –∞—Å–∏–º–ø—Ç–æ—Ç–∏–∫: –ê–ª–æ–≥—Ä–∏—Ç–º—ã –ù–û–î (2 –≤–∏–¥–∞), –ù–û–ö.
#include <iostream>

void NOD1(int a, int b) {
  while (a != 0 && b != 0) {
    if (a > b)
      a -= b;
    else
      b -= a;
  }
  std::cout << a + b;
}

int NOD2(int a, int b) {
  while (a * b != 0) {
    if (a > b)
      a %= b;
    else
      b %= a;
  }
  return a + b;
}

int NOK(int a, int b) {
  return a * b / NOD2(a, b);
}

int main() {
  int a, b;
  std::cin >> a >> b;
  //NOD1(a, b);
  //NOD2(a, b);
  std::cout << NOK(a, b);
}
